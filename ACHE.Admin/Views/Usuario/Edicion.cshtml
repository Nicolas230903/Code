@model ACHE.Admin.Models.UsuarioViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="pageheader">
    <h2>
        <i class="fa fa-users"></i>Usuarios
        <span>Administración</span>
    </h2>
</div>

<div class="contentpanel">

    <div class="row mb15">
        <div class="col-sm-12" id="divDatosPersonales">
            <!-- profile-header -->
            <div class="alert alert-danger" id="divError" style="display: none">
                <strong>Lo sentimos! </strong><span id="msgError"></span>
            </div>
            <div class="alert alert-success" id="divOk" style="display: none">
                <strong>Bien hecho! </strong>Los datos se han actualizado correctamente
            </div>

            <ul class="nav nav-tabs nav-justified nav-profile">

                <li id="liPerfilUsuario" class="active" runat="server">
                    <a href="#PerfilUsuario" data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-smile-o" style="font-size: 18px;"></span>
                        <strong>PERFIL Y USUARIOS</strong><br />

                    </a>
                </li>
                <li id="liplanPagos" runat="server">
                    <a href="#planPagos" data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-credit-card" style="font-size: 18px;"></span>
                        <strong>PLAN Y PAGOS</strong><br />

                    </a>
                </li>
                <li id="liEmpresas" runat="server">
                    <a href="#Empresas" data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-briefcase" style="font-size: 18px;"></span>
                        <strong>EMPRESAS</strong><br />

                    </a>
                </li>
                <li id="liEstadisticas" runat="server">
                    <a href="#Estadisticas" data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-bar-chart" style="font-size: 18px;"></span>
                        <strong>ESTADÍSTICAS</strong><br />

                    </a>
                </li>
                <li id="liDatosInternos" runat="server">
                    <a href="#DatosInternos"  data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-user" style="font-size: 18px;"></span>
                        <strong>Datos internos</strong><br />

                    </a>
                </li>
                <li id="liAccesos" runat="server">
                    <a href="#Accesos" data-toggle="tab" style="text-align: left;">
                        <span class="fa fa-check" style="font-size: 18px;"></span>
                        <strong>Accesos</strong><br />

                    </a>
                </li>
                <li id="li4"><a href="#" data-toggle="tab"><strong></strong></a></li>
                <li id="li1"><a href="#" data-toggle="tab"><strong></strong></a></li>
            </ul>
            <form id="frmEdicion" runat="server" clientidmode="Static">
                <div class="tab-content">
                    <div class="tab-pane active" id="PerfilUsuario" runat="server">
                        <ul class="nav nav-tabs nav-justified nav-profile tab-content" id="tabsPerfil">
                            <li id="liDatosPrincipales" class="active"><a href="#info" data-toggle="tab" style="text-align: left;"><strong>Datos principales</strong></a></li>
                            <li id="liDomicilio"><a href="#domicilio" data-toggle="tab" style="text-align: left;"><strong>Domicilio fiscal</strong></a></li>
                            <li id="liDatosFiscales"><a href="#datosFiscales" data-toggle="tab" style="text-align: left;"><strong>Datos impositivos</strong></a></li>
                            @*<li id="liportalClientes" runat="server"><a href="#portalClientes" data-toggle="tab" style="text-align: left;"><strong>Acceso clientes</strong></a></li>*@
                            <li id="liTemplate" runat="server"><a href="#template" data-toggle="tab" style="text-align: left;"><strong>Formato de factura</strong></a></li>
                            <li id="liCambiarpwd"><a href="#" onclick="usuario.resetearPwd(@Model.ID,'@Model.RazonSocial');" style="text-align: left;"><strong>Resetear contraseña</strong></a></li>
                            <li id="liImportaciones"><a href="#" onclick="usuario.importaciones(@Model.ID);" style="text-align: left;"><strong>Importar datos</strong></a></li>
                            <li id="liUsuariosAd"><a href="#UsuariosAd" data-toggle="tab" style="text-align: left;"><strong>Usuarios</strong></a></li>
                            <li id="liConfiguracion"><a href="#configuracion" data-toggle="tab" style="text-align: left;"><strong>Configuración</strong></a></li>
                            <li id="liFijarNuevaClave"><a href="#fijarNuevaClave" data-toggle="tab" style="text-align: left;"><strong>Fijar nueva clave</strong></a></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="info">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="col-sm-12">
                                            <div class="col-sm-2" id="divFotoPerfil">

                                                @if (!string.IsNullOrWhiteSpace(Model.Logo))
                                                {
                                                    <img id="imgLogo" src="http://app.contabilium.com/files/usuarios/@Model.Logo" class="thumbnail img-responsive" alt="" runat="server" />
                                                }
                                                else
                                                {
                                                    <img id="imgLogo" src="http://app.contabilium.com/files/usuarios/no-photo.png" class="thumbnail img-responsive" alt="" runat="server" />
                                                }

                                                <div class="col-sm-12" id="divEliminarUsuario" style="padding-top: 20px">
                                                    <a class="btn btn-danger" onclick="usuario.eliminarUsuario(@Model.ID);">ELIMINAR USUARIO</a>
                                                </div>
                                                <div class="col-sm-12 alert alert-success" id="divOkEliminarUsuario" style="display: none">
                                                    <strong></strong><span id="msgOkEliminarUsuario"></span>
                                                </div>
                                                <div class="col-sm-12 alert alert-danger" id="divErrorEliminarUsuario" style="display: none">
                                                    <strong>Lo sentimos! </strong><span id="msgErrorEliminarUsuario"></span>
                                                </div>

                                                <div class="mb30"></div>

                                                <div id="divLogo" style="display: none">
                                                    <p class="mb30">Formato JPG, PNG o GIF. Tamaño máximo recomendado: 200x60px</p>
                                                    <input type="hidden" value="" name="" /><input type="file" id="flpArchivo" />
                                                    <div class="mb20"></div>
                                                </div>

                                            </div>
                                            <div class="col-sm-1"></div>
                                            <div class="col-sm-9">
                                                <div class="row mb15">
                                                    <div class="col-sm-12">
                                                        <h3>Datos principales </h3>
                                                        <label class="control-label">Estos son los datos básicos y obligatorios que el sistema necesita para poder facturar.</label>
                                                    </div>
                                                </div>
                                                <div class="row mb15">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label"><code>*</code> Razón Social</label>
                                                            @Html.TextBoxFor(m => m.RazonSocial, new { @class = "form-control required", @id = "txtRazonSocial", @maxlength = "128", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label"><code>*</code> Categoría Impositiva</label>
                                                            @Html.DropDownListFor(m => m.CondicionIva, new SelectList(new List<Object>{
                                                                                                new { value = "" , text = "" },
                                                                                                new { value = "Ex" , text = "Exento" },
                                                                                                new { value = "RI" , text = "Responsable Inscripto"  },
                                                                                                new { value = "NI" , text = "Responsable no inscripto" },
                                                                                                new { value = "MO" , text = "Monotributista"  }
                                                            },
                                                          "value",
                                                          "text"), new { @class = "select2", @id = "ddlCondicionIva", @placeholder = "Seleccione un valor" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mb15">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label"><code>*</code> CUIT</label>
                                                            @Html.TextBoxFor(m => m.CUIT, new { @class = "form-control required number validCuit", @id = "txtCuit", @maxlength = "13", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label"><code>*</code> E-mail</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">@@</span>
                                                                @Html.TextBoxFor(m => m.Email, new { @class = "form-control required", @id = "txtEmail", @maxlength = "128", @type = "email" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mb15">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Inicio de actividades</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                                                                @Html.TextBoxFor(m => m.FechaInicioActividades, new { @class = "form-control validDate", @id = "txtFechaInicioAct", @maxlength = "10", @disabled = "true" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label"><code>*</code> Personería</label>
                                                            @Html.DropDownListFor(m => m.Personeria, new SelectList(new List<Object>{
                                                                                                    new { value = "F" , text = "Física" },
                                                                                                    new { value = "J" , text = "Jurídica"  }
                                                                },
                                                              "value",
                                                              "text"), new { @class = "form-control", @id = "ddlPersoneria", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mb15">
                                                    <div class="col-sm-12">
                                                        <hr />
                                                        <h3>Datos generales </h3>
                                                        <label class="control-label">Estos datos no son necesarios para facturar, pero podrían serte de utilidad ;-)</label>
                                                    </div>
                                                </div>

                                                <div class="row mb15">
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label class="control-label">IIBB</label>
                                                            @Html.CheckBoxFor(m => m.TieneFacturaElectronica, new { @id = "chkExento", @Text = "&nbsp;Soy exento", @Style = "border: 0; background-color: transparent;", @disabled = "true" })

                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label class="control-label">Nro. IIBB</label>
                                                            @Html.TextBoxFor(m => m.ExentoIIBB, new { @class = "form-control number", @id = "txtIIBB", @maxlength = "15", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">E-mail alertas</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon">@@</span>
                                                                @Html.TextBoxFor(m => m.EmailAlertas, new { @class = "form-control", @id = "txtEmailAlertas", @disabled = "true" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row mb15">
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Contacto</label>
                                                            @Html.TextBoxFor(m => m.Contacto, new { @class = "form-control", @id = "txtContacto", @maxlength = "200", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">Celular</label>
                                                            <div class="input-group">
                                                                <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                                                @Html.TextBoxFor(m => m.Celular, new { @class = "form-control", @id = "txtCelular", @maxlength = "50" })
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row mb15">
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label class="control-label">Es contador</label>
                                                            @Html.CheckBoxFor(m => m.EsContador, new { @id = "chkEsContador", @Text = "&nbsp;Es contador", @Style = "border: 0; background-color: transparent;", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label class="control-label">Usa plan corporativo</label>
                                                            @Html.CheckBoxFor(m => m.UsaPlanCorporativo, new { @id = "chkUsaPlanCorporativo", @Text = "&nbsp;Usa Plan de Corporativo", @Style = "border: 0; background-color: transparent;", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel-footer">
                                        <div class="col-sm-3"></div>
                                        <div class="col-sm-9">
                                            <a class="btn btn-success" id="btnActualizarInfo" onclick="usuario.grabar(@Model.ID);">Actualizar</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="domicilio">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="row mb15">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span class="asterisk">*</span> Provincia</label>
                                                    @*<select id="ddlProvincia" class="select2 required" data-placeholder="Selecciona una provincia...">
                                                        </select>*@
                                                    @Html.TextBoxFor(m => m.ProvinciaNombre, new { @class = "form-control", @id = "ddlProvincia", @maxlength = "100", @disabled = "true" })
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span class="asterisk">*</span> Ciudad/Localidad</label>
                                                    @*<select id="ddlCiudad" class="select2 required" data-placeholder="Selecciona una ciudad...">
                                                        </select>*@
                                                    @Html.TextBoxFor(m => m.CiudadNombre, new { @class = "form-control", @id = "ddlCiudad", @maxlength = "100", @disabled = "true" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row mb15">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span class="asterisk">*</span> Domicilio</label>
                                                    @Html.TextBoxFor(m => m.Domicilio, new { @class = "form-control required", @id = "txtDomicilio", @maxlength = "100", @disabled = "true" })
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">Piso/Depto</label>
                                                    @Html.TextBoxFor(m => m.PisoDepto, new { @class = "form-control", @id = "txtPisoDepto", @maxlength = "10", @disabled = "true" })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row mb15">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">Código Postal</label>
                                                    @Html.TextBoxFor(m => m.CodigoPostal, new { @class = "form-control", @id = "txtCp", @maxlength = "10", @disabled = "true" })
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label">Teléfono</label>
                                                    <div class="input-group">
                                                        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
                                                        @Html.TextBoxFor(m => m.Telefono, new { @class = "form-control", @id = "txtTelefono", @maxlength = "50", @textmode = "Phone", @disabled = "true" })
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel-footer hide">
                                        <a class="btn btn-success" id="btnActualizarDomicilio" onclick="MisDatos.grabar();">Actualizar</a>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="portalClientes">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <h3>¿Quiere que sus clientes accedan a un portal para descargar las facturas que les emitís? (VÁLIDO para plan PYME en adelante)</h3>
                                        <p class="mb30">
                                            Seleccionando "Habilitar acceso de clientes" sis clientes podrán ingresar a <a href="http://clientes.contabilium.com" target="_blank">http://clientes.contabilium.com</a> y descargar todas sus facturas de forma online.
                                        </p>

                                        <div class="row mb15">
                                            <div class="col-sm-6">
                                                <div class="form-group">
                                                    @Html.CheckBoxFor(m => m.PortalClientes, new { @id = "chkPortalClientes", @disabled = "true" })
                                                    <label class="control-label">&nbsp;Habilitar acceso de clientes</label>
                                                </div>

                                                <div class="form-group">
                                                    @Html.CheckBoxFor(m => m.CorreoPortal, new { @id = "ChkCorreoPortal", @disabled = "true" })
                                                    <label class="control-label">&nbsp;Enviar mail automáticamente al emitirles una factura</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div>
                                            <hr />
                                            <h3>¿Quiere cobrarle a sus clientes las facturas que le emite a través de MercadoPago?</h3>
                                            <p>Sigue los siguientes pasos:</p>
                                            <br />
                                            <p>
                                                1.- Ingresa a <a href="https://www.mercadopago.com/mla/herramientas/aplicaciones" target="_blank">www.mercadopago.com/mla/herramientas/aplicaciones</a>
                                            </p>
                                            <p>
                                                2.- Completa el usuario y contraseña de Mercado Pago en caso de ser necesario.
                                            </p>
                                            <p>
                                                3.- Copia los valores de Client_id y Client_secret a los campos correspondientes.
                                            </p>

                                            <div>
                                                <div class="row">
                                                    <div class="col-sm-9 col-md-5 col-lg-3">
                                                        <div class="form-group">
                                                            <label class="control-label">Client ID</label>
                                                            @Html.TextBoxFor(m => m.MercadoPagoClientID, new { @class = "form-control", @id = "txtClientId", @maxlength = "150", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-sm-9 col-md-5 col-lg-3">
                                                        <div class="form-group">
                                                            <label class="control-label">Client Secret</label>
                                                            @Html.TextBoxFor(m => m.MercadoPagoClientSecret, new { @class = "form-control", @id = "txtClientSecret", @maxlength = "150", @disabled = "true" })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <p>
                                                4 - Ingresa a <a href="https://www.mercadopago.com/mla/herramientas/notificaciones" target="_blank">www.mercadopago.com/mla/herramientas/notificaciones</a> y completa el campo Url con el siguiente valor: <strong style="color: #000">
                                                    https://clientes.contabilium.com/mercadopago.ashx?token=@Model.ApiKey
                                                </strong>
                                            </p>
                                        </div>
                                    </div>

                                    <div class="panel-footer hide">
                                        <a class="btn btn-success" id="btnActualizarPortalClientes" onclick="MisDatos.portalClientes();">Actualizar</a>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane" id="template">
                                <div class="panel-body">
                                    <h3>La imagen es todo</h3>

                                    <p>El diseño de factura elegido es.</p>

                                    <div class="row mb15">
                                        <div class="row filemanager">
                                            <div class="col-xs-6 col-sm-4 col-md-2">
                                                <div class="thmb">
                                                    <div class="thmb-prev">
                                                        <img src="~/images/templates/1.png" class="img-responsive" alt="" />
                                                    </div>
                                                    <h5 class="fm-title">
                                                        @Html.RadioButtonFor(m => m.TemplateFc, "default", new { @id = "default1", @name = "dlltemplate", @disabled = "true" })
                                                        Default
                                                    </h5>

                                                </div>
                                            </div>
                                            <div class="col-xs-6 col-sm-4 col-md-2">
                                                <div class="thmb">
                                                    <div class="thmb-prev">
                                                        <img src="~/images/templates/2.png" class="img-responsive" alt="" />
                                                    </div>
                                                    <h5 class="fm-title">
                                                        @Html.RadioButtonFor(m => m.TemplateFc, "amarillo", new { @id = "default2", @name = "dlltemplate", @disabled = "true" })
                                                        Amarillo
                                                    </h5>

                                                </div>
                                            </div>
                                            <div class="col-xs-6 col-sm-4 col-md-2">
                                                <div class="thmb">
                                                    <div class="thmb-prev">
                                                        <img src="~/images/templates/3.png" class="img-responsive" alt="" />
                                                    </div>
                                                    <h5 class="fm-title">
                                                        @Html.RadioButtonFor(m => m.TemplateFc, "celeste", new { @id = "default3", @name = "dlltemplate", @disabled = "true" })
                                                        Celeste
                                                    </h5>
                                                </div>
                                            </div>
                                            <div class="col-xs-6 col-sm-4 col-md-2">
                                                <div class="thmb">
                                                    <div class="thmb-prev">
                                                        <img src="~/images/templates/5.png" class="img-responsive" alt="" />
                                                    </div>
                                                    <h5 class="fm-title">
                                                        @Html.RadioButtonFor(m => m.TemplateFc, "rojo", new { @id = "default5", @name = "dlltemplate", @disabled = "true" })
                                                        Rojo
                                                    </h5>

                                                </div>
                                            </div>
                                            <div class="col-xs-6 col-sm-4 col-md-2">
                                                <div class="thmb">
                                                    <div class="thmb-prev">
                                                        <img src="~/images/templates/6.png" class="img-responsive" alt="" />
                                                    </div>
                                                    <h5 class="fm-title">
                                                        @Html.RadioButtonFor(m => m.TemplateFc, "verde", new { @id = "default6", @name = "dlltemplate", @disabled = "true" })
                                                        Verde
                                                    </h5>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-footer hide">
                                    <a class="btn btn-success" id="btnActualizarTemplate" onclick="MisDatos.ActualizarTemplate();">Actualizar</a>
                                </div>
                            </div>
                            <div class="tab-pane" id="datosFiscales">
                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="row mb15">
                                            <div class="col-sm-12">
                                                <h3>Puntos de venta </h3>
                                                <label class="control-label">Estos son los Puntos de ventas habilitados para facturar.</label>
                                            </div>
                                        </div>
                                        <div id="puntos">
                                            <div class="alert alert-danger" id="divErrorPuntos" style="display: none">
                                                <strong>Lo sentimos! </strong><span id="msgErrorPuntos"></span>
                                            </div>
                                            <br />
                                            <div class="table-responsive">
                                                <table class="table mb30">
                                                    <thead>
                                                        <tr>
                                                            <th>#</th>
                                                            <th>Punto</th>
                                                            <th>Fecha alta</th>
                                                            <th>Fecha baja</th>
